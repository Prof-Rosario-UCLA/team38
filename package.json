{
  "name": "sportsdashboard",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "dev:api": "npm run dev --prefix backend/express-api",
    "dev:dynamodb": "java -Djava.library.path=backend/dynamodb_local_latest/DynamoDBLocal_lib -jar backend/dynamodb_local_latest/DynamoDBLocal.jar -sharedDb",
    "dev:all": "concurrently -k -n FE,API,DB -c green,cyan,magenta \"npm:dev\" \"npm:dev:api\" \"npm:dev:dynamodb\"",
    "start": "node backend/express-api/dist/index.js",
    "build": "cd backend/express-api/ && tsc && cd ../.. && tsc -b && vite build",
    "lint": "eslint .",
    "preview": "vite preview"
  },
  "dependencies": {
    "@auth0/auth0-react": "^2.3.0",
    "@aws-sdk/client-dynamodb": "^3.826.0",
    "@aws-sdk/lib-dynamodb": "^3.826.0",
    "@emotion/react": "^11.14.0",
    "@emotion/styled": "^11.14.0",
    "@mui/icons-material": "^7.1.1",
    "@mui/material": "^7.1.1",
    "@types/date-fns": "^2.5.3",
    "@types/react-router-dom": "^5.3.3",
    "axios": "^1.9.0",
    "concurrently": "^9.1.2",
    "cors": "^2.8.5",
    "date-fns": "^4.1.0",
    "dotenv": "^16.5.0",
    "express": "^5.1.0",
    "react": "^19.1.0",
    "react-dom": "^19.1.0",
    "react-router-dom": "^7.6.0"
  },
  "devDependencies": {
    "@eslint/js": "^9.25.0",
    "@types/react": "^19.1.2",
    "@types/react-dom": "^19.1.2",
    "@vitejs/plugin-react": "^4.4.1",
    "eslint": "^9.25.0",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-react-refresh": "^0.4.19",
    "globals": "^16.0.0",
    "typescript": "^5.8.3",
    "typescript-eslint": "^8.30.1",
    "vite": "^6.3.5"
  }
}
